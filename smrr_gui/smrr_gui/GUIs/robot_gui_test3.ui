   <string notr="true">/* Main Window Background */
QMainWindow {
    background-color: #F7F9FC;  /* Soft Light Background */
    color: #2E3440;
}

/* General Widget Background */
QWidget {
    background: #FFFFFF;  
    border-radius: 10px;
}

/* Stacked Widget */
QStackedWidget {
    background: #FFFFFF;
    border-radius: 10px;
    padding: 10px;
}

/* Buttons */
QPushButton {
    background-color: #4A90E2;  /* Soft Blue */
    color: white;
    font-size: 16px;
    padding: 8px 12px;
    border-radius: 8px;
    border: 2px solid #357ABD;
    transition: 0.3s;
}

QPushButton:hover {
    background-color: #357ABD;
    border: 2px solid #4A90E2;
}

/* Labels */
QLabel {
    font-size: 18px;
    color: #2E3440;
    font-weight: bold;
}

/* Line Edit (Text Input) */
QLineEdit {
    background-color: #E3EAF2;
    color: #2E3440;
    border: 1px solid #A0AEC0;
    border-radius: 5px;
    padding: 5px;
}

/* Styled Scrollbars */
QScrollBar:vertical {
    border: none;
    background: #F1F5F9;
    width: 10px;
    margin: 0px 0px 0px 0px;
}

QScrollBar::handle:vertical {
    background: #A0AEC0;
    min-height: 20px;
    border-radius: 5px;
}

QScrollBar::add-line:vertical, 
QScrollBar::sub-line:vertical {
    background: none;
}

/* ComboBox */
QComboBox {
    background-color: #FFFFFF;
    border: 1px solid #A0AEC0;
    border-radius: 5px;
    padding: 5px;
}

QComboBox::drop-down {
    border: none;
}

QComboBox:hover {
    border: 1px solid #4A90E2;
}
</string>
